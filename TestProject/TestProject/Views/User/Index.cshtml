                <th>학점</th>
                <th>연도</th>
            </tr>
            @foreach (MSC msc in Model.Item6)
            {
                <tr>
                    <td>@msc.class_num</td>
                    <td>@msc.class_name</td>
                    <td>@msc.credit</td>
                    <td>@msc.year</td>
                </tr>
            }
        </table>
    }
    @if (Model.Item7.Count() > 0)
    {
        <hr />
        <table cellpadding="0" cellspacing="0" border="1">
            <tr>
                <th>학수번호</th>
