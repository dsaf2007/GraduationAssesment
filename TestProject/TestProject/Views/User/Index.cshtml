                <th>학수번호</th>
                <th>과목명</th>
                <th>학점</th>
                <th>연도</th>
            </tr>
            @foreach (BasicLiberalArts basicLiberal in Model.Item3)
            {
                <tr>
                    <td>@basicLiberal.class_num</td>
                    <td>@basicLiberal.class_name</td>
                    <td>@basicLiberal.credit</td>
                    <td>@basicLiberal.year</td>
                </tr>
            }
        </table>
    }
    @if (Model.Item4.Count() > 0)
    {
        <hr />
        <table cellpadding="0" cellspacing="0" border="1">
            <tr>
                <th>학수번호</th>
                <th>과목명</th>
                <th>학점</th>
                <th>연도</th>
            </tr>
            @foreach (BasicKnowledge basicKnowledge in Model.Item4)
            {
                <tr>
                    <td>@basicKnowledge.class_num</td>
                    <td>@basicKnowledge.class_name</td>
                    <td>@basicKnowledge.credit</td>
                    <td>@basicKnowledge.year</td>
                </tr>
            }
        </table>
    }
    @if (Model.Item5.Count() > 0)
    {
        <hr />
        <table cellpadding="0" cellspacing="0" border="1">
            <tr>
                <th>학수번호</th>
                <th>과목명</th>
                <th>학점</th>
                <th>연도</th>
            </tr>
            @foreach (ScienceExperiment scienceExperiment in Model.Item5)
            {
                <tr>
                    <td>@scienceExperiment.class_num</td>
